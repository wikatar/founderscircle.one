# Frontend Environment Variables
VITE_FORM_TOKEN=your_github_personal_access_token

# Serverless Function Environment Variables
GITHUB_TOKEN=your_github_personal_access_token

# GitHub Actions Secrets (not used in frontend)
EMAIL_USER=your_email@gmail.com
EMAIL_APP_PASSWORD=your_gmail_app_password
NOTIFICATION_EMAIL=notifications@yourdomain.com

# Format must be exactly: owner/repository-name (e.g., wikatar/FoundersCircle.one)
GITHUB_REPOSITORY=your_username/your_repo_name 